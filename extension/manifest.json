{
  "manifest_version": 3,
  "name": "Kaggle AI Project Helper",
  "version": "0.1",
  "description": "Generates project ideas from Kaggle dataset pages.",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "https://www.kaggle.com/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png"
  }
}